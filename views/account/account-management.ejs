<% if (title) { %>
<h1><%= title %></h1>
<% } %>

<%- messages() %>

<h2 id="welcome-message">Welcome <%= locals.accountData.account_firstname %></h2>

<p class="message">You're logged in</p>

<a href="/account/update/<%= accountData.account_id %>">Edit Account Information</a>  

<% if (locals.accountData.account_type === "Employee" || locals.accountData.account_type === "Admin") { %> 
  <div class="inventory-management-menu">
    <h3>Inventory Management</h3>
    <a href="/inv/">Manage Inventory</a>
  </div>
<% } %>

